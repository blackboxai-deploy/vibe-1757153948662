# QD-Enhanced Multi-LLM Swarm Agent System - Environment Variables

# Application Configuration
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME="QD-Enhanced Multi-LLM Swarm Agent System"
NEXT_PUBLIC_APP_VERSION=1.0.0

# Authentication Configuration
AUTH_SECRET=your-super-secure-jwt-secret-key-here
AUTH_TOKEN_EXPIRY=24h
SESSION_TIMEOUT=86400000

# Hardcoded Authentication Credentials (As Specified)
AUTH_USERNAME=egor1993
AUTH_PASSWORD=Nvidia980@
AUTH_ADMIN_ROLE=admin

# HashiCorp Vault Configuration
VAULT_ENDPOINT=https://your-vault-instance.com:8200
VAULT_TOKEN=your-vault-root-token
VAULT_NAMESPACE=your-vault-namespace
VAULT_MOUNT_PATH=secret
VAULT_APPROLE_ROLE_ID=your-approle-role-id
VAULT_APPROLE_SECRET_ID=your-approle-secret-id

# LLM Provider API Keys (Store in Vault for production)
OPENAI_API_KEY=your-openai-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here
GOOGLE_API_KEY=your-google-gemini-api-key-here
BLACKBOX_API_KEY=your-blackbox-ai-api-key-here
OPENROUTER_API_KEY=your-openrouter-api-key-here

# Pinecone Vector Database
PINECONE_API_KEY=your-pinecone-api-key-here
PINECONE_ENVIRONMENT=your-pinecone-environment
PINECONE_INDEX_NAME=qd-swarm-knowledge

# Custom LLM Endpoint Configuration (Pre-configured for demo)
CUSTOM_LLM_ENDPOINT=https://oi-server.onrender.com/chat/completions
CUSTOM_LLM_CUSTOMER_ID=cus_Sxr3rlGaox3rKZ
CUSTOM_LLM_AUTHORIZATION=Bearer xxx

# Database Configuration (Optional - for production persistence)
DATABASE_URL=postgresql://username:password@localhost:5432/qd_swarm_db
REDIS_URL=redis://localhost:6379
MONGODB_URI=mongodb://localhost:27017/qd-swarm

# Security & Encryption
ENCRYPTION_KEY=your-32-character-encryption-key-here
BCRYPT_SALT_ROUNDS=12
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=900000

# Agent Configuration
MAX_AGENTS=7
AGENT_MEMORY_LIMIT=1000
AGENT_REFLECTION_INTERVAL=600000
KNOWLEDGE_TRANSFER_THRESHOLD=0.7

# QD-Enhanced Features
QD_MEMORY_CONSOLIDATION=true
QD_CAUSAL_ANALYSIS=true
QD_ADAPTIVE_PLANNING=true
QD_EMERGENT_BEHAVIOR_DETECTION=true
QD_CROSS_AGENT_LEARNING=true
QD_SEMANTIC_SEARCH=true

# Performance & Monitoring
PERFORMANCE_TRACKING=true
AUDIT_LOGGING=true
METRICS_COLLECTION=true
HEALTH_CHECK_INTERVAL=30000

# Deployment Configuration
DEPLOYMENT_ENVIRONMENT=development
CORS_ORIGIN=http://localhost:3000
API_TIMEOUT=30000
MAX_REQUEST_SIZE=10mb

# Monitoring & Analytics (Optional)
SENTRY_DSN=your-sentry-dsn-for-error-tracking
ANALYTICS_ID=your-analytics-tracking-id
LOG_LEVEL=info
ENABLE_DEBUG=false

# WebSocket Configuration (For real-time features)
WS_PORT=3001
WS_HEARTBEAT_INTERVAL=30000
WS_MAX_CONNECTIONS=1000

# Cache Configuration
CACHE_TTL=3600
CACHE_MAX_SIZE=100
ENABLE_CACHING=true

# Development Tools
ENABLE_SWAGGER=true
ENABLE_GRAPHQL_PLAYGROUND=false
ENABLE_HOT_RELOAD=true

# Production Overrides (Set these in production)
# NODE_ENV=production
# NEXT_PUBLIC_APP_URL=https://your-production-domain.com
# DATABASE_URL=your-production-database-url
# REDIS_URL=your-production-redis-url
# VAULT_ENDPOINT=https://your-production-vault.com:8200